{
  "name": "tripToGo",
  "version": "1.0.0",
  "description": "Simple application to showcase how to achieve universal rendering and routing with React and Express.",
  "main": "server/server.js",
  "repository": "git@github.com:lmammino/judo-heroes.git",
  "scripts": {
    "start": "cross-env NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 server/server.js",
    "debug": "cross-env NODE_ENV=production node_modules/.bin/babel-node --debug --presets react,es2015 server/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "author": "Team 51",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^3.1.4",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.30.1",
    "jsx": "^0.9.89",
    "material-ui": "^0.20.0",
    "mongodb": "^3.0.1",
    "node-fetch": "^2.1.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "reactstrap": "^5.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "react-hot-loader": "^1.3.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.11.1",
    "babel-preset-es2015": "^6.13.2"
  }
}
